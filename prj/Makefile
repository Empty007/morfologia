clean :
	(cd src/lib; make clean)
	(cd src/obj; rm -f libprj.a)
	(cd src/vunibas; rm -f vunibas; rm -f *~)
	(cd src/vdilbas; rm -f vdilbas; rm -f *~)
	(cd src/vdspbas; rm -f vdspbas; rm -f *~)
	(cd src/vdspfact; rm -f vdspfact; rm -f *~)
	(cd src/verobas; rm -f verobas; rm -f *~)
	(cd src/vfactor; rm -f vfactor; rm -f *~)
	(cd src/vintbas; rm -f vintbas; rm -f *~)
	(cd src/vunibas; rm -f vnegbas; rm -f *~)
	(cd src/vsepbas; rm -f vsepbas; rm -f *~)
	(cd src/vgerbas; rm -f vgerbas; rm -f *~)
	(cd src/vronbas; rm -f vronbas; rm -f *~)
	(cd src/veqlbas; rm -f veqlbas; rm -f *~)
	(cd src/vdstbas; rm -f vdstbas; rm -f *~)
	(cd src/vastbas; rm -f vastbas; rm -f *~)
	(cd src/vstrger; rm -f str_ger; rm -f *~)
	(cd src/vsolbas; rm -f vlosbas; rm -f *~)
	(cd bin; rm -f strger;rm -f str_ger; rm -f *~)

make all:
	(cd src/lib; make libprj.a)
	(cd src/strger; make)
	(cd src/vdilbas; . vdil)
	(cd src/vdspbas; . monta)
	(cd src/vdspfact; . monta)
	(cd src/verobas; . monta)
	(cd src/vfactor; . monta)
	(cd src/vgerbas; make)
	(cd src/vintbas; . monta)
	(cd src/vnegbas; . monta)
	(cd src/vronbas; make)
	(cd src/vsepbas; . monta)
	(cd src/vunibas; . monta)
	(cd src/veqlbas; make)
	(cd src/vdstbas; make)
	(cd src/vastbas; make)
	(cd src/vstrger; make)
	(cd src/vsolbas; make vlosbas)
	(cd src/lib; make clean)

